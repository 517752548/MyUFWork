DROP TABLE IF EXISTS `t_cdkey`;
-- ----------------------------
-- Table structure for t_cdkey
-- ----------------------------
CREATE TABLE `t_cdkey` (
  `id` varchar(100) NOT NULL,
  `plansId` int(11) NOT NULL,
  `giftId` int(11) NOT NULL,
  `groupId` int(11) NOT NULL,
  `gmId` varchar(255) NOT NULL,
