  `chartServerId` varchar(255) default NULL,
  `takeTime` bigint(20) default '0',
  `isDel` int(11) NOT NULL default '0',
  PRIMARY KEY  (`id`),
  UNIQUE KEY `cdkey` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
DROP TABLE IF EXISTS `t_cdkey_plans`;
-- ----------------------------
-- Table structure for t_cdkey_plans
-- ----------------------------
