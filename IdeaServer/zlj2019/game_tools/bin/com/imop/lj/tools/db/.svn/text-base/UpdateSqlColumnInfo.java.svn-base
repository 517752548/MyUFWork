package com.imop.lj.tools.db;

public class UpdateSqlColumnInfo {
	private String name = "";
	private int len;
	private String defV = "";
	
	private boolean notNull;
	
	public UpdateSqlColumnInfo(String name, int len, boolean notNull, String defV) {
		this.name = name;
		this.len = len;
		this.notNull = notNull;
		this.defV = defV;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getLen() {
		return len;
	}
	public void setLen(int len) {
		this.len = len;
	}
	public String getDefV() {
		return defV;
	}
	public void setDefV(String defV) {
		this.defV = defV;
	}

	public boolean isNotNull() {
		return notNull;
	}

	public void setNotNull(boolean notNull) {
		this.notNull = notNull;
	}
	
}
