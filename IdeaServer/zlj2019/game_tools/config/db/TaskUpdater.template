package com.imop.lj.gameserver.${lowModelName}.persistance;

import com.imop.lj.core.object.PersistanceObject;
import com.imop.lj.gameserver.common.db.POUpdater;
import com.imop.lj.gameserver.${lowModelName}.${modelName};

public class ${updaterName} implements POUpdater {
	@Override
	public void delete(PersistanceObject<?, ?> obj) {
		final ${modelName} task = (${modelName})obj;
		${dbManagerName}.getInstance().delTaskEntity(task, true);
	}

	@Override
	public void save(PersistanceObject<?, ?> obj) {
		final ${modelName} task = (${modelName})obj;
		${dbManagerName}.getInstance().saveTaskEntity(task, true);
	}
}
