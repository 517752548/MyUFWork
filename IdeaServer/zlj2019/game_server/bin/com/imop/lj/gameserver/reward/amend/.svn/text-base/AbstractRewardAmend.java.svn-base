package com.imop.lj.gameserver.reward.amend;

import com.imop.lj.gameserver.reward.IRewardAmend;
import com.imop.lj.gameserver.reward.RewardDef.RewardAddedFromType;

/**
 * 抽象奖励加成
 * 
 * @author xiaowei.liu
 * 
 */
public abstract class AbstractRewardAmend implements IRewardAmend {
	private RewardAddedFromType type;
	
	public AbstractRewardAmend(RewardAddedFromType type){
		this.type = type;
	}

	@Override
	public RewardAddedFromType getAddedType() {
		return type;
	}

	
}
