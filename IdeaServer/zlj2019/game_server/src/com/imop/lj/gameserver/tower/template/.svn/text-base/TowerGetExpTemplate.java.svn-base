package com.imop.lj.gameserver.tower.template;

import com.imop.lj.core.annotation.ExcelRowBinding;
import com.imop.lj.core.template.BeanFieldNumber;

@ExcelRowBinding
public class TowerGetExpTemplate {
	//每层对应的经验值
	@BeanFieldNumber(number = 1)
	private long exp;

	public long getExp() {
		return exp;
	}

	public void setExp(long exp) {
		this.exp = exp;
	}

	@Override
	public String toString() {
		return "TowerGetExpTemplate [exp=" + exp + "]";
	}
}
