package com.imop.lj.gameserver.activityui;

import com.imop.lj.common.model.reward.RewardInfo;

public class ActivityUIInfo {
	
	private int activityId;
	private int activityType;
	private int activityTimes;
	private int specialType;
	private int finishStatue;
	private RewardInfo rewardInfo;
	private long cd; //限时活动的倒计时,毫秒

	public ActivityUIInfo(int activityId, int activityType, int activityTimes,
			int specialType, int finishStatue, RewardInfo rewardInfo, long cd) {
		super();
		this.activityId = activityId;
		this.activityType = activityType;
		this.activityTimes = activityTimes;
		this.specialType = specialType;
		this.finishStatue = finishStatue;
		this.rewardInfo = rewardInfo;
		this.cd = cd;
	}
	public ActivityUIInfo() {
		super();
	}
	public RewardInfo getRewardInfo() {
		return rewardInfo;
	}
	public void setRewardInfo(RewardInfo rewardInfo) {
		this.rewardInfo = rewardInfo;
	}
	public int getFinishStatue() {
		return finishStatue;
	}
	public void setFinishStatue(int finishStatue) {
		this.finishStatue = finishStatue;
	}
	public int getActivityId() {
		return activityId;
	}
	public void setActivityId(int activityId) {
		this.activityId = activityId;
	}
	public int getActivityType() {
		return activityType;
	}
	public void setActivityType(int activityType) {
		this.activityType = activityType;
	}
	public int getActivityTimes() {
		return activityTimes;
	}
	public void setActivityTimes(int activityTimes) {
		this.activityTimes = activityTimes;
	}

	public int getSpecialType() {
		return specialType;
	}

	public void setSpecialType(int specialType) {
		this.specialType = specialType;
	}
	public long getCd() {
		return cd;
	}
	public void setCd(long cd) {
		this.cd = cd;
	}
	@Override
	public String toString() {
		return "ActivityUIInfo [activityId=" + activityId + ", activityType=" + activityType + ", activityTimes="
				+ activityTimes + ", specialType=" + specialType + ", finishStatue=" + finishStatue + ", rewardInfo="
				+ rewardInfo + ", cd=" + cd + "]";
	}
	
}
