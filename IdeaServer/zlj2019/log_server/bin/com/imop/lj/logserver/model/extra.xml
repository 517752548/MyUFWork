<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
          "http://www.ibatis.com/dtd/sql-map-2.dtd" >
<sqlMap>
    <statement id="dropTable" parameterClass="java.lang.String">
    	DROP TABLE IF EXISTS $tableName$;
    </statement>
    
     <select id="selectTableName" parameterClass="java.util.Map" resultClass="java.lang.String">
    	select table_name from information_schema.tables where table_name like '$tableName$%' and table_schema = '$database$';
    </select>
</sqlMap>