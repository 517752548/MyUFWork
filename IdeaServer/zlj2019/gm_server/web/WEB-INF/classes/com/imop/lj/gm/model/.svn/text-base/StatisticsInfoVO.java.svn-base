package com.imop.lj.gm.model;

/**
 * 统计信息VO
 * 
 * @author xiaowei.liu
 * 
 */
public class StatisticsInfoVO {
	private boolean state;
	private String svrName;
	private int registerNum;
	private int createRoleNum;
	private int chargeRoleNum;
	private long totalCharge;
	private long todayCharge;
	private long weekCharge;
	private long monthCharge;
	private String onlinePlayerSize;
	private String cdr;
	private String cdc;
	private double arup;

	public String getSvrName() {
		return svrName;
	}

	public void setSvrName(String svrName) {
		this.svrName = svrName;
	}

	public int getRegisterNum() {
		return registerNum;
	}

	public void setRegisterNum(int registerNum) {
		this.registerNum = registerNum;
	}

	public int getCreateRoleNum() {
		return createRoleNum;
	}

	public void setCreateRoleNum(int createRoleNum) {
		this.createRoleNum = createRoleNum;
	}

	public int getChargeRoleNum() {
		return chargeRoleNum;
	}

	public void setChargeRoleNum(int chargeRoleNum) {
		this.chargeRoleNum = chargeRoleNum;
	}

	public long getTotalCharge() {
		return totalCharge;
	}

	public void setTotalCharge(long totalCharge) {
		this.totalCharge = totalCharge;
	}

	public String getCdr() {
		return cdr;
	}

	public void setCdr(String cdr) {
		this.cdr = cdr;
	}

	public String getCdc() {
		return cdc;
	}

	public void setCdc(String cdc) {
		this.cdc = cdc;
	}

	public double getArup() {
		return arup;
	}

	public void setArup(double arup) {
		this.arup = arup;
	}

	public boolean isState() {
		return state;
	}

	public void setState(boolean state) {
		this.state = state;
	}

	public long getTodayCharge() {
		return todayCharge;
	}

	public void setTodayCharge(long todayCharge) {
		this.todayCharge = todayCharge;
	}

	public long getWeekCharge() {
		return weekCharge;
	}

	public void setWeekCharge(long weekCharge) {
		this.weekCharge = weekCharge;
	}

	public long getMonthCharge() {
		return monthCharge;
	}

	public void setMonthCharge(long monthCharge) {
		this.monthCharge = monthCharge;
	}

	public String getOnlinePlayerSize() {
		return onlinePlayerSize;
	}

	public void setOnlinePlayerSize(String onlinePlayerSize) {
		this.onlinePlayerSize = onlinePlayerSize;
	}
}
