@echo off

set jar_lib=config
SetLocal EnableDelayedExpansion

for %%i in ("..\lib\*.jar") do set jar_lib=!jar_lib!;%%i

start javaw -Dserver.name=${server.serverName}_${server.id} -server -Xmx1024M -Xms512M -Xss128K -XX:NewRatio=2 -XX:PermSize=64m -XX:MaxPermSize=64m -XX:+UseConcMarkSweepGC -cp %jar_lib% ${server.mainClass} 

EndLocal