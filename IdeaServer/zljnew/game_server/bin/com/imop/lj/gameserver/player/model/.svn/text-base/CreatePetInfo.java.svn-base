package com.imop.lj.gameserver.player.model;

public class CreatePetInfo {

	/** 主将模板id */
	private int petTemplateId;
	/** 主将头像id */
	private int petPhotoId;
	/** 主将职业类型 */
	private int petJobType;
	/** 主将职业名称 */
	private String petJobName;
	/** 主将职业描述 */
	private String petDesc;

	public int getPetTemplateId() {
		return petTemplateId;
	}

	public void setPetTemplateId(int petTemplateId) {
		this.petTemplateId = petTemplateId;
	}

	public int getPetPhotoId() {
		return petPhotoId;
	}

	public void setPetPhotoId(int petPhotoId) {
		this.petPhotoId = petPhotoId;
	}

	public int getPetJobType() {
		return petJobType;
	}

	public void setPetJobType(int petJobType) {
		this.petJobType = petJobType;
	}

	public String getPetJobName() {
		return petJobName;
	}

	public void setPetJobName(String petJobName) {
		this.petJobName = petJobName;
	}

	public String getPetDesc() {
		return petDesc;
	}

	public void setPetDesc(String petDesc) {
		this.petDesc = petDesc;
	}
}
