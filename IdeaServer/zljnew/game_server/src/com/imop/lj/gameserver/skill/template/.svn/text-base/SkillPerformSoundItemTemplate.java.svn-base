package com.imop.lj.gameserver.skill.template;

import com.imop.lj.core.annotation.ExcelRowBinding;
import com.imop.lj.core.template.BeanFieldNumber;

@ExcelRowBinding
public class SkillPerformSoundItemTemplate {
	/**
	 * 音效id
	 */
	@BeanFieldNumber(number = 1)
	private String soundId;
	
	/**
	 * 音效开始播放的时间
	 */
	@BeanFieldNumber(number = 2)
	private float soundStartTime;
	
	/**
	 * 音效是否循环播放
	 */
	@BeanFieldNumber(number = 3)
	private int isLoop;

	/**
	 * @return the soundId
	 */
	public String getSoundId() {
		return soundId;
	}

	/**
	 * @param soundId the soundId to set
	 */
	public void setSoundId(String soundId) {
		this.soundId = soundId;
	}

	/**
	 * @return the soundStartTime
	 */
	public float getSoundStartTime() {
		return soundStartTime;
	}

	/**
	 * @param soundStartTime the soundStartTime to set
	 */
	public void setSoundStartTime(float soundStartTime) {
		this.soundStartTime = soundStartTime;
	}

	/**
	 * @return the soundLoopTimes
	 */
	public int getIsLoop() {
		return isLoop;
	}

	/**
	 * @param soundLoopTimes the soundLoopTimes to set
	 */
	public void setIsLoop(int isLoop) {
		this.isLoop = isLoop;
	}
}
