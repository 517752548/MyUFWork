package com.imop.lj.common.model.template;

import com.imop.lj.core.annotation.ExcelRowBinding;
import com.imop.lj.core.template.BeanFieldNumber;

/**
 * 激励模版
 * 
 * @author xiaowei.liu
 * 
 */
@ExcelRowBinding
public class EncourageTemplate {
	/** 激励类型 */
	@BeanFieldNumber(number = 1)
	private int recourgeTypeId;
	/** 激励值 */
	@BeanFieldNumber(number = 2)
	private int recourgeValue;

	public int getRecourgeTypeId() {
		return recourgeTypeId;
	}

	public void setRecourgeTypeId(int recourgeTypeId) {
		this.recourgeTypeId = recourgeTypeId;
	}

	public int getRecourgeValue() {
		return recourgeValue;
	}

	public void setRecourgeValue(int recourgeValue) {
		this.recourgeValue = recourgeValue;
	}

}
