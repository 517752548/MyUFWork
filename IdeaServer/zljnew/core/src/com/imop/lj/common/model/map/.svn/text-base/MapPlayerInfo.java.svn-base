package com.imop.lj.common.model.map;

public class MapPlayerInfo {
	/** 玩家唯一Id */
	private long uuid;
	
	/** 1删除，2移动，3添加，4更新 */
	private int msgType;
	
	private String name = "";
	private int level;
	private String model = "";
	
	private int mapId;
	private int tileX;
	private int tileY;
	private int x;
	private int y;
	
	private int isLeader;
	private int isFighting;
	
	private int rideHorseTplId;
	
	private String extStr = "";

	private String titleName = "";
	
	private int isForaging;
	
	private int wingTplId;
	
	private int fashionTplId;
	
	private long corpsId;
	
	private int equipWeaponId;
	
	private int vipLevel;
	
	/** 0未在采集,1-伐木,2-采药,3-采矿,4-狩猎*/
	private int lifeSkillFlag;

	public long getUuid() {
		return uuid;
	}

	public void setUuid(long uuid) {
		this.uuid = uuid;
	}

	public int getMsgType() {
		return msgType;
	}

	public void setMsgType(int msgType) {
		this.msgType = msgType;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getLevel() {
		return level;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public String getModel() {
		return model;
	}

	public void setModel(String model) {
		this.model = model;
	}

	public int getMapId() {
		return mapId;
	}

	public void setMapId(int mapId) {
		this.mapId = mapId;
	}

	public int getTileX() {
		return tileX;
	}

	public void setTileX(int tileX) {
		this.tileX = tileX;
	}

	public int getTileY() {
		return tileY;
	}

	public void setTileY(int tileY) {
		this.tileY = tileY;
	}

	public String getExtStr() {
		return extStr;
	}

	public void setExtStr(String extStr) {
		this.extStr = extStr;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

	public int getIsLeader() {
		return isLeader;
	}

	public void setIsLeader(int isLeader) {
		this.isLeader = isLeader;
	}

	public int getIsFighting() {
		return isFighting;
	}

	public void setIsFighting(int isFighting) {
		this.isFighting = isFighting;
	}

	public int getRideHorseTplId() {
		return rideHorseTplId;
	}

	public void setRideHorseTplId(int rideHorseTplId) {
		this.rideHorseTplId = rideHorseTplId;
	}

	public String getTitleName(){
		return titleName;
	}
    public void setTitleName(String titleName){
        this.titleName = titleName;
    }
    
	public int getIsForaging() {
		return isForaging;
	}

	public void setIsForaging(int isForaging) {
		this.isForaging = isForaging;
	}

	public long getCorpsId() {
		return corpsId;
	}

	public void setCorpsId(long corpsId) {
		this.corpsId = corpsId;
	}
	
	public int getWingTplId() {
		return wingTplId;
	}

	public void setWingTplId(int wingTplId) {
		this.wingTplId = wingTplId;
	}
	
	public int getFashionTplId() {
		return fashionTplId;
	}

	public void setFashionTplId(int fashionTplId) {
		this.fashionTplId = fashionTplId;
	}

	public int getEquipWeaponId() {
		return equipWeaponId;
	}

	public void setEquipWeaponId(int equipWeaponId) {
		this.equipWeaponId = equipWeaponId;
	}

	public int getVipLevel() {
		return vipLevel;
	}

	public void setVipLevel(int vipLevel) {
		this.vipLevel = vipLevel;
	}
	
	public int getLifeSkillFlag() {
		return lifeSkillFlag;
	}

	public void setLifeSkillFlag(int lifeSkillFlag) {
		this.lifeSkillFlag = lifeSkillFlag;
	}

	@Override
	public String toString() {
		return "MapPlayerInfo [uuid=" + uuid + ", msgType=" + msgType + ", name=" + name + ", level=" + level
				+ ", model=" + model + ", mapId=" + mapId + ", tileX=" + tileX + ", tileY=" + tileY + ", x=" + x
				+ ", y=" + y + ", isLeader=" + isLeader + ", isFighting=" + isFighting + ", rideHorseTplId="
				+ rideHorseTplId + ", extStr=" + extStr + ", titleName=" + titleName + ", isForaging=" + isForaging
				+ ", wingTplId=" + wingTplId + ", fashionTplId=" + fashionTplId + ", corpsId=" + corpsId
				+ ", equipWeaponId=" + equipWeaponId + ", vipLevel=" + vipLevel + ", lifeSkillFlag=" + lifeSkillFlag
				+ "]";
	}
}
