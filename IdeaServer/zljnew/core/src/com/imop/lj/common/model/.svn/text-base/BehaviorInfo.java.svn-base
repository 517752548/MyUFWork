package com.imop.lj.common.model;


public class BehaviorInfo {
	private int bType;
	private int bIndex;
	private int max;

	public BehaviorInfo() {
		
	}
	
	public BehaviorInfo(int bType, int bIndex, int max) {
		this.bType = bType;
		this.bIndex = bIndex;
		this.max = max;
	}

	public int getBType() {
		return bType;
	}

	public void setBType(int bType) {
		this.bType = bType;
	}

	public int getBIndex() {
		return bIndex;
	}

	public void setBIndex(int bIndex) {
		this.bIndex = bIndex;
	}

	public int getMax() {
		return max;
	}

	public void setMax(int max) {
		this.max = max;
	}

}
