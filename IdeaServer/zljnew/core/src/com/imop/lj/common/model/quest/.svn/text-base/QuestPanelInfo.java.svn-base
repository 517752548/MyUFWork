package com.imop.lj.common.model.quest;

public class QuestPanelInfo {

	/**
	 * 任务类型
	 */
	private int questType;
	/**
	 * 任务等级限制
	 */
	private int questMinLevel;
	/**
	 * 今日已完成任务数
	 */
	private int finishTimes;

	/**
	 * 总任务数
	 */
	private int totalTimes;

	public int getQuestType() {
		return questType;
	}

	public void setQuestType(int questType) {
		this.questType = questType;
	}

	public int getQuestMinLevel() {
		return questMinLevel;
	}

	public void setQuestMinLevel(int questMinLevel) {
		this.questMinLevel = questMinLevel;
	}

	public int getFinishTimes() {
		return finishTimes;
	}

	public void setFinishTimes(int finishTimes) {
		this.finishTimes = finishTimes;
	}

	public int getTotalTimes() {
		return totalTimes;
	}

	public void setTotalTimes(int totalTimes) {
		this.totalTimes = totalTimes;
	}

	@Override
	public String toString() {
		return "QuestPanelInfo [questType=" + questType + ", questMinLevel=" + questMinLevel + ", finishTimes="
				+ finishTimes + ", totalTimes=" + totalTimes + "]";
	}
	
}
