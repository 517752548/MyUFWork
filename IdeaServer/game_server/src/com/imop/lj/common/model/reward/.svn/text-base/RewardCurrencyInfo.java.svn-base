package com.imop.lj.common.model.reward;

public class RewardCurrencyInfo {
	private int currencyId;

	private int amount;

	public int getCurrencyId() {
		return currencyId;
	}

	public void setCurrencyId(int currencyId) {
		this.currencyId = currencyId;
	}

	public int getAmount() {
		return amount;
	}

	public void setAmount(int amount) {
		this.amount = amount;
	}

	@Override
	public String toString() {
		return "RewardCurrencyInfo [currencyId=" + currencyId + ", amount="
				+ amount + "]";
	}

}
