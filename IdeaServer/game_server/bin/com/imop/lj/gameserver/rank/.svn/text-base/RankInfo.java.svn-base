package com.imop.lj.gameserver.rank;

public class RankInfo {
	private Integer rank;
	private String humanName;
	private String petName;
	private String  corpsName;
	private Integer level;
	private Integer humanJob;
	private Integer fightPower;
	private Integer score;
	private Long humanId;
	private Long petId;
	private Long levelUpTimeStamp;
	
	public RankInfo() {
		super();
		this.rank = -1;
		this.humanName = "";
		this.petName = "";
		this.corpsName = "";
		this.level = -1;
		this.humanJob = -1;
		this.fightPower = -1;
		this.score = -1;
		this.humanId = -1L;
		this.petId = -1L;
		this.levelUpTimeStamp = -1L;
	}
	
	public RankInfo(RankInfo ri) {
		super();
		this.rank = ri.getRank();
		this.humanName = ri.getHumanName();
		this.petName = ri.getPetName();
		this.corpsName = ri.getCorpsName();
		this.level = ri.getLevel();
		this.humanJob = ri.getHumanJob();
		this.fightPower = ri.getFightPower();
		this.score = ri.getScore();
		this.humanId = ri.getHumanId();
		this.petId = ri.getPetId();
		this.levelUpTimeStamp = ri.getLevelUpTimeStamp();
	}
	
	public Integer getRank() {
		return rank;
	}
	public void setRank(Integer rank) {
		this.rank = rank;
	}
	public String getHumanName() {
		return humanName;
	}
	public void setHumanName(String humanName) {
		this.humanName = humanName;
	}
	public String getPetName() {
		return petName;
	}
	public void setPetName(String petName) {
		this.petName = petName;
	}
	public String getCorpsName() {
		return corpsName;
	}
	public void setCorpsName(String corpsName) {
		this.corpsName = corpsName;
	}
	public Integer getLevel() {
		return level;
	}
	public void setLevel(Integer level) {
		this.level = level;
	}
	public Integer getHumanJob() {
		return humanJob;
	}
	public void setHumanJob(Integer humanJob) {
		this.humanJob = humanJob;
	}
	public Integer getFightPower() {
		return fightPower;
	}
	public void setFightPower(Integer fightPower) {
		this.fightPower = fightPower;
	}
	public Integer getScore() {
		return score;
	}
	public void setScore(Integer score) {
		this.score = score;
	}
	public Long getHumanId() {
		return humanId;
	}
	public void setHumanId(Long humanId) {
		this.humanId = humanId;
	}
	public Long getPetId() {
		return petId;
	}
	public void setPetId(Long petId) {
		this.petId = petId;
	}
	public Long getLevelUpTimeStamp() {
		return levelUpTimeStamp;
	}
	public void setLevelUpTimeStamp(Long levelUpTimeStamp) {
		this.levelUpTimeStamp = levelUpTimeStamp;
	}
	
}
