namespace app.role
{
    public abstract class Role : AbsMonoBehaviour
    {
        /// <summary>
        /// 角色类型
        /// </summary>
        protected RoleTypes roleType;
        public RoleTypes RoleType
        {
            get { return roleType; }
        }

        public Role(RoleTypes roleType)
