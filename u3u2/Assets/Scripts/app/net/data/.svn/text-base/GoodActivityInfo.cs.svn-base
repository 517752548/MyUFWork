
namespace app.net
{
	public class GoodActivityInfo
	{
		/** 活动唯一Id */
		public long activityId;
		/** 活动类型Id */
		public int typeId;
		/** 活动图标 */
		public int icon;
		/** 名称图标 */
		public int nameIcon;
		/** 标题图标 */
		public int titleIcon;
		/** 名称 */
		public string name;
		/** 描述  */
		public string desc;
		/** 是否新活动，1新，0否 */
		public int isNew;
		/** 活动开始时间 */
		public long startTime;
		/** 活动结束时间 */
		public long endTime;
		/** 是否有未领取的奖励，1是，0否 */
		public int hasUnGotBonus;
		/** 倒计时描述 */
		public string countDownTimeDesc;
		/** 倒计时时间 */
		public long countDownTime;
		/** 自身相关信息 */
		public string selfInfo;
		/** 活动目标json串 */
		public string targetInfo;
		/** 目标类型，前端显示用 */
		public int showTargetType;
		/** 最近开启的 */
		public int isRecentOpen;
		/** 最近结束的 */
		public int isRecentClose;
		/** 日志列表  */
		public string[] logList;
		/** 是否需要隐藏，0否，1是 */
		public int needHide;
	}
}