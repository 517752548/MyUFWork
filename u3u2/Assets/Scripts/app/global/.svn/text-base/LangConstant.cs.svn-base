    public static string getPetPropertyName(int propId,bool trim=false)
    {
        string str = "属       性";
        if (propId > PetBProperty._BEGIN && propId <= (PetBProperty._BEGIN+PetBProperty._SIZE))
        {
            str= bProps[propId - PetBProperty._BEGIN - 1];
        }
        else if (propId > PetAProperty._BEGIN && propId <= (PetAProperty._BEGIN+PetAProperty._SIZE))
        {
            str = aProps[propId - PetAProperty._BEGIN - 1];
        }
        if (propId == RoleBaseIntProperties.LIFE)
        {
            str= "寿       命";
        }
        if (trim)
        {
            str = str.Replace(" ","");
        }
        return str;
    }
    

