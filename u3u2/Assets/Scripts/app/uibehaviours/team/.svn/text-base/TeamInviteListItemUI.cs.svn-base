using UnityEngine;
using UnityEngine.UI;

public class TeamInviteListItemUI : MonoBehaviour
{
	public Image icon;
	public Text nameTxt;
	public Text lvTxt;
	public Text careerTxt;
	public GameUUButton inviteBtn;
	public Text inviteBtnLabel;
    public void Init()
    {
        icon = transform.Find("Image 1/RawImage").GetComponent<Image>();
        nameTxt = transform.Find("name").GetComponent<Text>();
        lvTxt = transform.Find("lv").GetComponent<Text>();
        careerTxt = transform.Find("career").GetComponent<Text>();
        inviteBtn = transform.Find("inviteBtn").GetComponent<GameUUButton>();
        inviteBtnLabel = transform.Find("inviteBtn/Text").GetComponent<Text>();

    }
}
