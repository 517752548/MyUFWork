using UnityEngine;
using System.Collections;
using UnityEngine.UI;

public class PetHorseLianJiePackUI : MonoBehaviour 
{
    public Text m_petname;
    public Text m_pethorsename;
    public GameUUToggle m_check;
    public Image m_checkbg;

    public void Init()
    {
        m_petname = transform.Find("pet").GetComponent<Text>();
        m_pethorsename = transform.Find("pethorse").GetComponent<Text>();

        m_check = transform.Find("check").GetComponent<GameUUToggle>();
        m_checkbg = transform.Find("check/Background").GetComponent<Image>();
    }
}
