using UnityEngine;
using UnityEngine.UI;

public class BattleFrontSkillListUIBehav : MonoBehaviour
{
    public GameUUButton[] btns = new GameUUButton[5];
    public Image[] icons = new Image[5];
    public void Init()
    {
        btns[0] = transform.Find("1").GetComponent<GameUUButton>();
        btns[1] = transform.Find("2").GetComponent<GameUUButton>();
        btns[2] = transform.Find("3").GetComponent<GameUUButton>();
        btns[3] = transform.Find("4").GetComponent<GameUUButton>();
        btns[4] = transform.Find("5").GetComponent<GameUUButton>();

        icons[0] = transform.Find("1/icon").GetComponent<Image>();
        icons[1] = transform.Find("2/icon").GetComponent<Image>();
        icons[2] = transform.Find("3/icon").GetComponent<Image>();
