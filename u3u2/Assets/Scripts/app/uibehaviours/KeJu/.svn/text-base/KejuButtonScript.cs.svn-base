using UnityEngine;
using System.Collections;
using UnityEngine.UI;

public class KejuButtonScript : MonoBehaviour
{

    public GameUUButton kejuAnswerBtn;

    public Text buttonText;

    public Image rightImage;

    public Image wrongImage;

    public void Init()
    {
        kejuAnswerBtn = transform.GetComponent<GameUUButton>();
        buttonText = transform.Find("Text").GetComponent<Text>();
        rightImage = transform.Find("rightImage").GetComponent<Image>();
        wrongImage = transform.Find("wrongImage").GetComponent<Image>();
    }
}
