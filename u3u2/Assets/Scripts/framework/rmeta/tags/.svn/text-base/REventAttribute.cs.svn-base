using System;    [AttributeUsage (AttributeTargets.Class, AllowMultiple = true, Inherited = false)]    public class REventAttribute:Attribute    {        public REventAttribute()        {        }        private String _type;        public String type        {            get            {                return _type;            }            set            {                _type = value;            }        }        public String _handler;        public String handler        {            get            {                return _handler;            }set            {                _handler = value;            }        }         }