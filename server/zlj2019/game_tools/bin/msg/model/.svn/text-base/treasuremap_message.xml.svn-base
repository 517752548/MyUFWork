<?xml version="1.0" encoding="UTF-8"?>
<messages xmlns="http://com.imop.lj.message" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://com.imop.lj.message message.xsd" module="treasuremap">

	<message type="GC_OPEN_TREASUREMAP_PANEL" comment="藏宝图">
		<field type="Integer" name="finishTimes" comment="今日已完成任务数" />
		<field type="Integer" name="totalTimes" comment="总任务数" />
	</message>

	<message type="GC_TREASUREMAP_DONE" comment="已完成所有任务">
	</message>
	
	<message type="GC_TREASUREMAP_UPDATE" comment="除暴安良任务更新消息">
		<field type="com.imop.lj.common.model.quest.QuestInfo"
			clientType="QuestInfoData" name="questInfo" comment="单个任务" macro="QuestInfo">
		</field>
	</message>
	
	<message type="CG_TREASUREMAP_ACCEPT" comment="接受任务">
	</message>
	
	<message type="CG_GIVE_UP_TREASUREMAP" comment="放弃已接任务">
	</message>
	
	<message type="CG_FINISH_TREASUREMAP" comment="完成任务">
	</message>

</messages>