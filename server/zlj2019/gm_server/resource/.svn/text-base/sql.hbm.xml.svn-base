	<query name="getAllCDKeyList">
		<![CDATA[ from CDKeyEntity where isDel = 0 order by plansId, giftId, groupId  ]]>
	</query>
	<query name="getCDKeyEntityListByGroupId">
		<![CDATA[ from CDKeyEntity where plansId=:plansId and giftId=:giftId and groupId=:groupId and state = 0 ]]>
	</query>
	<query name="getCDKeyListByCDKeyId">
		<![CDATA[ from CDKeyEntity where id like :id ]]>
	</query>
	<query name="getCDKeyListByCDKeyIdOrCreateTime">
		<![CDATA[ from CDKeyEntity where cdkey like :id or createTime=:createTime ]]>
	</query>
	<query name="getCDKeyEntityListByOpenId">
		<![CDATA[ from CDKeyEntity where openId like:openId]]>
	</query>
	<query name="getMaxGroupId">
		<![CDATA[ select max(groupId) from CDKeyEntity where plansId=:plansId and giftId =:giftId ]]>
	</query>
	<query name="getWorldGift" >
		<![CDATA[ from WorldGiftEntity where isDel = 0 order by id ]]>
	</query>
	<query name="queryWorldGiftByGiftId"> 
		<![CDATA[ from WorldGiftEntity where giftId=:giftId ]]>
	</query>
	<query name="queryCDKeyPlansByPlansId"> 
		<![CDATA[ from CDKeyPlansEntity where cdkeyPlansId=:plansId ]]>
	</query>
	<query name="getCDKeyPlans"> 
		<![CDATA[ from CDKeyPlansEntity where isDel = 0 order by id ]]>
	</query>
	<query name="getByPlansNameOrCreateTime">
		<![CDATA[ from CDKeyPlansEntity where cdkeyPlansName like :plansName or (createTime>:createTimeStart and createTime <:createTimeEnd) ]]>
	</query>
	<query name="getByPlansName">
		<![CDATA[ from CDKeyPlansEntity where cdkeyPlansName like :plansName ]]>
	</query>
	<query name="getAllCDKeyListNoEcho">
		<![CDATA[ from CDKeyEntity where isDel = 0 and state = 0 group by plansId, giftId, groupId order by plansId, giftId,groupId ]]>
	</query>
	<query name="delCDKey"> 
		<![CDATA[ update CDKeyEntity set isDel = 1 where plansId=:plansId and giftId=:giftId and groupId=:groupId ]]>
	</query>
</hibernate-mapping>