<hibernate-mapping>
<resultset name="queryUserInfo">
		<return-scalar  column="id" type="string"  />
		<return-scalar column="name" type="string" />
		<return-scalar column="password" type="string" />
		<return-scalar column="question" type="string" />
		<return-scalar column="answer" type="string" />
		<return-scalar column="lastLoginIp" type="string" />
		<return-scalar column="lastLogoutTime" type="timestamp" />
		<return-scalar column="role" type="int" />
		<return-scalar column="lockStatus" type="int" />
		<return-scalar column="props" type="string" />
		<return-scalar column="foribedTalkTime" type="long" />
</resultset>
<resultset name="queryHumanEntity">
		<return-scalar  column="id" type="long"  />
		<return-scalar column="passportId" type="string" />
		<return-scalar column="level" type="int" />
		<return-scalar column="lastLoginTime" type="timestamp" />
		<return-scalar column="lastLoginIp" type="string" />
		<return-scalar column="name" type="string" />
		<return-scalar column="onlineStatus" type="int" />
		<return-scalar column="createTime" type="timestamp" />
		<return-scalar column="bond" type="int" />
		<return-scalar column="sysBond" type="int" />
		<return-scalar column="honor" type="int" />
</resultset>
	<query name="getUserInfoList"> 
