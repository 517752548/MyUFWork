package com.imop.lj.common.model.mall;

import com.imop.lj.common.model.CurrencyInfo;
import com.imop.lj.common.model.item.CommonItem;

/**
 * 商城普通物品信息
 * 
 * @author xiaowei.liu
 * 
 */
public class MallNormalItemInfo {
	private int mallItemId;
	private CommonItem commonItem;
	private CurrencyInfo originalPrice;
	private CurrencyInfo discountPrice;
	private CurrencyInfo[] vipPrices;
	private int hot;
	private String marks;

	public int getMallItemId() {
		return mallItemId;
	}

	public void setMallItemId(int mallItemId) {
		this.mallItemId = mallItemId;
	}

	public CommonItem getCommonItem() {
		return commonItem;
	}

	public void setCommonItem(CommonItem commonItem) {
		this.commonItem = commonItem;
	}

	public CurrencyInfo getOriginalPrice() {
		return originalPrice;
	}

	public void setOriginalPrice(CurrencyInfo originalPrice) {
		this.originalPrice = originalPrice;
	}

	public CurrencyInfo getDiscountPrice() {
		return discountPrice;
	}

	public void setDiscountPrice(CurrencyInfo discountPrice) {
		this.discountPrice = discountPrice;
	}

	public CurrencyInfo[] getVipPrices() {
		return vipPrices;
	}

	public void setVipPrices(CurrencyInfo[] vipPrices) {
		this.vipPrices = vipPrices;
	}

	public String getMarks() {
		return marks;
	}

	public void setMarks(String marks) {
		this.marks = marks;
	}

	public int getHot() {
		return hot;
	}

	public void setHot(int hot) {
		this.hot = hot;
	}
}
