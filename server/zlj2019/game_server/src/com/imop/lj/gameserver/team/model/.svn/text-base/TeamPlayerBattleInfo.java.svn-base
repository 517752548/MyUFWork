package com.imop.lj.gameserver.team.model;

import com.imop.lj.gameserver.battle.pvp.PvpPlayerInfo;
import com.imop.lj.gameserver.battle.report.BattleReportAddition;

public class TeamPlayerBattleInfo extends PvpPlayerInfo {
	
	private BattleReportAddition bra;

	public TeamPlayerBattleInfo(long humanId, boolean isAttacker) {
		super(humanId, isAttacker);
	}

	public BattleReportAddition getBra() {
		return bra;
	}

	public void setBra(BattleReportAddition bra) {
		this.bra = bra;
	}

}
