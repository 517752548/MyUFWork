package com.imop.lj.gameserver.cd;

/**
 * 冷却队列信息
 *
 * @author haijiang.jin
 *
 */
public class CdQueueInfo {
	/** 冷却队列类型 */
	private int cdTypeInt;
	/** 冷却队列名称 */
	private String name;
	/** 图标 */
	private String icon;
	/** 索引 */
	private int index;
	/** 时间差 */
	private int currTimeDiff;
	/** 是否已开启 */
	private boolean opened;
	/** 是否可以累计时间 */
	private boolean canAddTime;

	/**
	 * 获取冷却队列类型
	 *
	 * @return
	 */
	public int getCdTypeInt() {
		return cdTypeInt;
	}

	/**
	 * 设置冷却队列类型
	 *
	 * @param cdTypeInt
	 *
	 */
	public void setCdTypeInt(int cdTypeInt) {
		this.cdTypeInt = cdTypeInt;
	}

	/**
	 * 获取冷却队列名称
	 *
	 * @return
	 */
	public String getName() {
		return name;
	}

	/**
	 * 设置冷却队列名称
	 *
	 * @param name
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * 获取图标
	 *
	 * @return
	 */
	public String getIcon() {
		return this.icon;
	}

	/**
	 * 设置图标
	 *
	 * @param value
	 */
	public void setIcon(String value) {
		this.icon = value;
	}

	/**
	 * 获取冷却队列索引
	 *
	 * @return
	 */
	public int getIndex() {
		return index;
	}

	/**
	 * 设置冷却队列索引
	 *
	 * @param index
	 */
	public void setIndex(int index) {
		this.index = index;
	}

	/**
	 * 获取服务器当前时间与结束时间的时间差
	 *
	 * @return
	 */
	public int getCurrTimeDiff() {
		return currTimeDiff;
	}

	/**
	 * 设置服务器当前时间与结束时间的时间差
	 *
	 * @param currTimeDiff
	 */
	public void setCurrTimeDiff(int currTimeDiff) {
		this.currTimeDiff = currTimeDiff;
	}

	/**
	 * 获取是否已经开启
	 *
	 * @return
	 */
	public boolean getOpened() {
		return opened;
	}

	/**
	 * 设置是否已经开启
	 *
	 * @param opened
	 */
	public void setOpened(boolean opened) {
		this.opened = opened;
	}

	/**
	 * 获取是否可以累计时间
	 *
	 * @return
	 */
	public boolean getCanAddTime() {
		return canAddTime;
	}

	/**
	 * 设置是否已经冷却
	 *
	 * @param canAddTime
	 */
	public void setCanAddTime(boolean canAddTime) {
		this.canAddTime = canAddTime;
	}
}
