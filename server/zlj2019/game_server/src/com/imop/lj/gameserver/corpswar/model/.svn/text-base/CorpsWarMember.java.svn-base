package com.imop.lj.gameserver.corpswar.model;

import com.imop.lj.gameserver.corps.CorpsDef.MemberJob;

public class CorpsWarMember {
	//玩家id
	private long roleId;
	//玩家军团id
	private long corpsId;
	//玩家积分
	private int score;
	//参加战斗次数
	private int fightNum;
	//战斗胜利次数
	private int winNum;
	//帮派职位
	private MemberJob job;
	
	public CorpsWarMember(long roleId, long corpsId, int score, MemberJob job) {
		this.roleId = roleId;
		this.corpsId = corpsId;
		this.score = score;
		this.job = job;
	}

	public long getRoleId() {
		return roleId;
	}

	public void setRoleId(long roleId) {
		this.roleId = roleId;
	}

	public long getCorpsId() {
		return corpsId;
	}

	public void setCorpsId(long corpsId) {
		this.corpsId = corpsId;
	}

	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score = score;
	}

	public int getFightNum() {
		return fightNum;
	}

	public void setFightNum(int fightNum) {
		this.fightNum = fightNum;
	}

	public int getWinNum() {
		return winNum;
	}

	public void setWinNum(int winNum) {
		this.winNum = winNum;
	}

	public MemberJob getJob() {
		return job;
	}

	public void setJob(MemberJob job) {
		this.job = job;
	}
}
