package com.imop.lj.gameserver.promote;

public class PromoteInfo {

	/** 提升Id,对应的是promote.xls模板中ID */
	private int protmoteId;
	/** 是否可以提升 */
	private boolean canPromote;
	
	public int getProtmoteId() {
		return protmoteId;
	}
	public void setProtmoteId(int protmoteId) {
		this.protmoteId = protmoteId;
	}

	public boolean getCanPromote() {
		return canPromote;
	}
	
	public void setCanPromote(boolean canPromote) {
		this.canPromote = canPromote;
	}
	
	@Override
	public String toString() {
		return "PromoteInfo [protmoteId=" + protmoteId + ", canPromote=" + canPromote + "]";
	}
	
	
}
