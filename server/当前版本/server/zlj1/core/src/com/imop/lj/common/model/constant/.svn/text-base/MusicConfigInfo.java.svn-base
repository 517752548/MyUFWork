package com.imop.lj.common.model.constant;

/**
 * 音乐配置
 * 
 * @author xiaowei.liu
 * 
 */
public class MusicConfigInfo {
	private int id;
	private int resId;
	private int loop;

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getResId() {
		return resId;
	}

	public void setResId(int resId) {
		this.resId = resId;
	}

	public int getLoop() {
		return loop;
	}

	public void setLoop(int loop) {
		this.loop = loop;
	}

}
