package com.imop.lj.common.model.corps;

/**
 * 详细军团信息
 * 
 * @author xiaowei.liu
 * 
 */
public class DetailCorpsInfo {
	/** 军团ID */
	private long corpsId;
	/** 军团名称 */
	private String name;
	/** 军团级别 */
	private int level;
	/** 是否有下一等级 */
	private int hasNextLevel;
	/** 当前成员数量 */
	private int currMemNum;
	/** 当前帮派经验 */
	private long currExp;
	/** 当前帮派资金 */
	private long currFund;
	/** 团长名称 */
	private String presidentName;
	/** 公告 */
	private String notice;
	/** 军团排名 */
	private int rank;
	/** 军团确认解散日期 */
	private long disbandConfirmDate;

	public long getCorpsId() {
		return corpsId;
	}

	public void setCorpsId(long corpsId) {
		this.corpsId = corpsId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getLevel() {
		return level;
	}

	public void setLevel(int level) {
		this.level = level;
	}

	public int getHasNextLevel() {
		return hasNextLevel;
	}

	public void setHasNextLevel(int hasNextLevel) {
		this.hasNextLevel = hasNextLevel;
	}
	
	public long getCurrExp() {
		return currExp;
	}

	public void setCurrExp(long currExp) {
		this.currExp = currExp;
	}

	public long getCurrFund() {
		return currFund;
	}

	public void setCurrFund(long currFund) {
		this.currFund = currFund;
	}

	public String getPresidentName() {
		return presidentName;
	}

	public void setPresidentName(String presidentName) {
		this.presidentName = presidentName;
	}

	public int getCurrMemNum() {
		return currMemNum;
	}

	public void setCurrMemNum(int currMemNum) {
		this.currMemNum = currMemNum;
	}

	public String getNotice() {
		return notice;
	}

	public void setNotice(String notice) {
		this.notice = notice;
	}

	public int getRank() {
		return rank;
	}

	public void setRank(int rank) {
		this.rank = rank;
	}

	public long getDisbandConfirmDate() {
		return disbandConfirmDate;
	}

	public void setDisbandConfirmDate(long disbandConfirmDate) {
		this.disbandConfirmDate = disbandConfirmDate;
	}

	@Override
	public String toString() {
		return "DetailCorpsInfo [corpsId=" + corpsId + ", name=" + name + ", level=" + level + ", hasNextLevel="
				+ hasNextLevel + ", currMemNum=" + currMemNum + ", currExp=" + currExp + ", currFund=" + currFund
				+ ", presidentName=" + presidentName + ", notice=" + notice + ", rank=" + rank + ", disbandConfirmDate="
				+ disbandConfirmDate + "]";
	}
	
}
