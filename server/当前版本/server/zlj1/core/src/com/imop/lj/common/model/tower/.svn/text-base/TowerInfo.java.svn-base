package com.imop.lj.common.model.tower;

/**
 * 通天塔信息 
 *
 */
public class TowerInfo {

	/** 当前玩家的通天塔层数*/
	private int curTowerLevel;
	/** 双倍点数*/
	private long curDoublePoint;
	/** 是否已开启,1:开启,0:未开启*/
	private int openDouble;
	public int getCurTowerLevel() {
		return curTowerLevel;
	}
	public void setCurTowerLevel(int curTowerLevel) {
		this.curTowerLevel = curTowerLevel;
	}
	public long getCurDoublePoint() {
		return curDoublePoint;
	}
	public void setCurDoublePoint(long curDoublePoint) {
		this.curDoublePoint = curDoublePoint;
	}
	public int getOpenDouble() {
		return openDouble;
	}
	public void setOpenDouble(int openDouble) {
		this.openDouble = openDouble;
	}
	@Override
	public String toString() {
		return "TowerInfo [curTowerLevel=" + curTowerLevel + ", curDoublePoint=" + curDoublePoint + ", openDouble="
				+ openDouble + "]";
	}
	
}
