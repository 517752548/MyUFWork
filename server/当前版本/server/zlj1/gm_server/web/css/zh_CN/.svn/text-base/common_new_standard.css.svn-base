#type-f {
	padding-left: 0px;
	padding-right: 0px;
	padding-top: 0px;
	padding-bottom: 0px;
	margin: 0px;
	background: #375A49 none repeat scroll 0 0;
	width: 100%;
	height: 100%;
}

select,input {
	background-color: #F9F9F9;
	border: 1px solid #CCCCCC;
	height: 20px;
}

body {
	padding-left: 0px;
	padding-right: 0px;
	padding-top: 0px;
	padding-bottom: 0px;
	margin: 0px;
	background-color: #FFFFFF;
	font-family: Verdana, Arial, Vrinda, Tahoma;
	font-size: 12px;
	color: #666;
	width: 100%;
	height: 100%;
}

a {
	color: #555;
	text-decoration: none;
}

a:hover {
	color: #0099CC;
}

a:visited {
	color: #660099;
}

img {
	border-top-width: 0px;
	border-right-width: 0px;
	border-bottom-width: 0px;
	border-left-width: 0px;
	display: inline;
}

ul {
	margin: 0;
	padding: 0;
	list-style-position: outside;
	list-style-type: none;
}

li {
	margin: 0;
	padding: 0;
}

div {
	margin: 0;
	padding: 0;
}

.header_content {
	margin: 0 auto;
	padding: 0px;
	width: 90%;
	font-size: 15px;
	background-color: #F3F8EF;
}

.header_content .logo {
	height: 100%;
	float: left;
	margin-left: 0px;
}

.header_content .right_nav {
	float: right;
	width: 70%;
	height: 50px;
}

.right_nav .text_left {
	float: left;
	width: 20%;
	padding: 2px 0 0 0;
	color: #CC3300;
}

.right_nav .text_right {
	float: right;
	width: 80%;
	display: inline;
	padding: 2px 0 0 0;
	text-align: right;
}

.nav_list a {
	display: inline;
	color: #3399CC;
}

.nav_list li {
	display: inline;
}

.nav_return a {
	display: inline;
	color: #3399CC;
}

.nav_return li {
	display: inline;
}

#left_content {
	clear: both;
	width: 100%;
	overflow-x: hidden;
	overflow: scroll;
	padding-bottom: 10px;
}

.list_tilte {
	background-color: #F1F1F1;
	border-bottom: 1px solid #CCCCCC;
	border-right: 1px solid #CCCCCC;
	font-weight: 900;
	cursor: pointer;
}

.list_tilte span {
	background: transparent url(../../images/dot1.gif) no-repeat scroll 5px
		8px;
	display: block;
	padding: 6px 0 6px 25px;
	text-decoration: none;
	width: 100%;
}

}
.list_detail {
	padding: 0px;
	background: #F8F8F8 none repeat scroll 0 0;
	border: 0 none;
	margin: 0;
	width: 100%;
}

.list_detail div {
	width: 100%;
}

.list_detail a {
	background: transparent url(../../images/dot3.gif) no-repeat scroll 5px
		4px;
	color: #444444;
	display: block;
	padding: 3px 0 3px 25px;
	text-decoration: none;
	width: 100%;
}

.list_detail a:hover {
	background: #508545 url(../../images/dot4.gif) no-repeat scroll 5px 4px;
	display: block;
	color: #FFFFFF;
	width: 100%;
}

table.sbmenu {
	border: 1px solid #F8F8F8;
	width: 100%;
	background-color: #F8F8F8;
}

table.sbmenu td {
	padding: 0px;
	margin: 0px;
	width: 100%;
}

#switchpic {
	width: 6px;
	cursor: pointer;
	clear: both;
	vertical-align: bottom;
	margin-top: 220px;
}

#nav {
	width: 100%;
	float: left;
	background: #EEFFEE none repeat scroll 0 0;
	height: 2em;
	white-space: nowrap;
	border-bottom: 1px solid #508545;
}

#nav ul {
	list-style: none;
}

#nav li {
	float: left;
	line-height: 24px;
	font-weight: bold;
}

.bg_image {
	color: #0099CC;
	cursor: pointer;
	border-bottom: 0px;
	padding: 2px 6px 15px 8px;
	z-index: 2;
}

.bg_image_onclick {
	background: white none repeat scroll 0 0;
	border-color: #508545;
	border-style: solid solid none;
	border-width: 1px 1px 0;
	margin: 1px -6px 2px 0px;
	padding: 2px 6px 15px 8px;
}

#sub_info {
	width: 99.8%;
	float: left;
	height: 27px;
	background-repeat: repeat-x;
	padding: 4px 0px 0 px 0px;
	background: White none repeat scroll 0 0;
	border: 1px solid #508545;
	border-top: 0px;
	display: inline;
}

input.limitWidth {
	width: 100px;
}

.left_title_1 {
	background-color: #F3F8F7;
	color: #73938E;
	font-weight: bold;
	line-height: 30px;
	text-align: right
}

.left_title_2 {
	background-color: #fff;
	color: #73938E;
	font-weight: bold;
	line-height: 30px;
	text-align: right;
}

table.welcome {
	border-right: #e8e8e8 1px solid;
	padding-right: 0px;
	border-top: #e8e8e8 1px solid;
	padding-left: 0px;
	font-size: 9pt;
	padding-bottom: 0px;
	vertical-align: top;
	border-left: #e8e8e8 1px solid;
	padding-top: 0px;
	border-bottom: #e8e8e8 1px solid;
	background-color: #fff;
	text-align: left;
	border-spacing: 0px;
}

table.welcome td.header {
	background-color: #508545;
	color: #FFFFFF;
	font-size: 9pt;
	font-weight: 600;
	height: 26px;
	padding-left: 5px;
	vertical-align: bottom;
}

table.welcome td.label {
	background-color: #F7F7F7;
	padding-left: 5px;
	border-right: 1px solid #B4C9C6;
}

table.welcome td {
	background-color: #fff;
	border-bottom: 1px solid #B4C9C6;
	padding-left: 5px;
	text-indent: 2em;
}

span.error_msg {
	color: red;
}

.red {
	color: red;
}

#show_text input {
	margin-left: 1em;
}

.link {
	color: #0099CC;
	text-decoration: none;
}

table.roleBasicInfo {
	width: 50%
}

table.roleBasicInfo td {
	width: 15%;
}

table.petBasicInfo {
	width: 80%
}

table.petBasicInfo td {
	width: 10%;
}

.tab-row {
	background: #EAF2FF none repeat scroll 0 0;
	height: 27px;
	white-space: nowrap;
	width: 100%;
	float: left;
	margin-top: 5px;
}

.selected {
	background: white none repeat scroll 0 0;
}

.tab {
	border-color: #508545;
	cursor: pointer;
	display: inline;
	border-style: none solid none none;
	border-width: 0 1px 0 0;
	float: left;
	font-family: Verdana, Helvetica, Arial;
	font-size: 12px;
	margin: 1px 0px;
	padding: 2px 6px;
	position: relative;
}

.tab a {
	color: #0042AE;
	cursor: pointer;
	font-family: Verdana, Helvetica, Arial;
	font-size: 13px;
	text-decoration: none;
}

.search {
	float: left;
	margin-top: 1em;
	width: 100%;
}

.search b {
	margin-left: 2em;
}

table.detail #del {
	width: 50px;
}

.pointer {
	cursor: pointer;
}

#sortImg {
	width: 12px;
	height: 12px;
}

#logintable {
	border: 1px solid #B4C9C6;
	background: #fff;
	margin-top: 2em;
}

#logintable td.region {
	background-color: #f3f8ef;
	border-bottom: 1px solid #668B8B;
	font-size: 12pt;
	font-weight: 900;
	padding: 5px;
}

#logintable td.label {
	background-color: #B4CDCD;
	border-bottom: 1px solid #668B8B;
	font-size: 12pt;
	font-weight: 900;
	padding: 5px;
}

div.nofloat {
	clear: both;
}

a.selected {
	color: #002BB8;
}

.pageBut {
	margin-top: 5px;
}

#num_style {
	text-align: right;
}

#num_style a {
	clear: both;
	background: #F7F9FF none repeat scroll 0;
	border: 1px solid #B2C9D3;
	color: #333333;
	font-size: 12px;
	margin-right: 3px;
	padding: 3px 6px 2px;
	text-decoration: none;
	height: 50px;
}

.curPage {
	color: red;
}

.pno {
	border: 1px solid rgb(122, 174, 189);
	width: 25px;
	height: 12px;
	font-size: 12px;
}

.time_gap {
	width: 80px;
}

.t_red {
	color: red
}

.tlnk {
	color: #fff
}

.tlnk select {
	font-size: 12px
}

.tlnk a {
	color: #304213
}

.tlnk a:hover {
	color: #fff
}

.topnav {
	width: 100%;
	background: #f5f5f5;
	border-top: 2px solid #ccc;
	line-height: 25px;
	text-indent: 15px;
	font-size: 14px
}

.topnav a,.topnav a:link {
	color: #063;
	font-size: 14px
}

.topnav a:visited {
	color: #333
}

.topnav a:hover {
	color: #F60
}

.topnav p {
	display: inline
}

.butcom {
	background: transparent url(../../images/bg-button.png) repeat scroll 0
		0;
	border: 1px solid #336666;
	color: #FFFFFF;
	cursor: pointer;
	font-size: 12px;
	line-height: 18px;
	margin-top: 3px;
}

table.detail td.bottom {
	background-color: #CCCCCC;
	border: 0 none;
	text-align: right;
	vertical-align: bottom;
	border-right: 1px solid #000;
}

table.detail {
	background-color: #FFFFFF;
	border:1px solid #000000;
	border-right:0px;
	border-spacing: 0;
	font-size: 9pt;
	margin-left: 1em;
	margin-top: 2em;
	padding: 0;
	vertical-align: top;
	text-align: center;
	width: 98%;
}

table.detail th {
	background-color: #CCCCCC;
	color: #000000;
	border-bottom: 1px solid #000;
	border-right: 1px solid #000;
	font-size: 9pt;
	font-weight: 600;
	height: 26px;
	padding-left: 5px;
}
table.detail th a{
	color:#0033FF;
	text-decoration: underline;
	cursor: pointer;
}

table.detail td {
	border-bottom: 1px solid #000000;
	border-right: 1px solid #000000;
	padding: 4px 1px;
}

}
table.detail td.label {
	background-color: #F7F7F7;
	padding-left: 5px;
	text-indent: 0;
}

table.detail td.header {
	background-color: #508545;
	color: #FFFFFF;
	font-size: 9pt;
	font-weight: 600;
	height: 26px;
	padding-left: 5px;
	vertical-align: bottom;
}

table.no_bottom {
	border-bottom: 0px;
}

table.task_page {
	margin-left: 1em;
	border: 1px solid #000;
	border-top: 0px;
}
#man_zone{
 width: 105%;
 height: 100%;
}
#reason{
  width: 150px;
}